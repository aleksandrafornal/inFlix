<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Production_year_can_not_be_in_future</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    NOT(ISBLANK(Production_Year__c)),
    Production_Year__c &gt; YEAR(TODAY())
)</errorConditionFormula>
    <errorDisplayField>Production_Year__c</errorDisplayField>
    <errorMessage>Production year can&apos;t be in future</errorMessage>
</ValidationRule>
